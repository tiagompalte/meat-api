{
  "name": "meat-api",
  "version": "1.0.0",
  "description": "API de restaurantes",
  "main": "index.js",
  "scripts": {
    "dev": "node_modules/.bin/nodemon --delay 5 dist/main.js",
    "gulp": "node_modules/.bin/gulp",
    "test": "ts-node src/jest.startup.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiagompalte/meat-api.git"
  },
  "keywords": [],
  "author": "Tiago Mariani Palte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiagompalte/meat-api/issues"
  },
  "homepage": "https://github.com/tiagompalte/meat-api#readme",
  "jest": {
    "globals": {
      "address": "http://localhost:3001",
      "auth": "Bearer "
    },
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "jsonwebtoken": "8.1.1",
    "mongoose": "4.13.9",
    "restify": "6.3.4",
    "restify-errors": "5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "1.0.0",
    "@types/jest": "22.1.2",
    "@types/jsonwebtoken": "7.2.5",
    "@types/mongoose": "4.7.32",
    "@types/restify": "5.0.6",
    "@types/restify-errors": "4.3.2",
    "@types/supertest": "2.0.4",
    "gulp": "^3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-typescript": "3.2.2",
    "jest": "22.4.2",
    "jest-cli": "22.4.2",
    "nodemon": "^1.17.5",
    "supertest": "3.0.0",
    "ts-jest": "22.0.4",
    "ts-node": "5.0.1",
    "typescript": "2.6.2"
  }
}
